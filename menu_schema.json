            "type": "array",
            "items": {
              "type": "object",
              "required": ["id", "name", "price", "quantity", "notes"],
              "properties": {
                "id": {
                  "type": "string"
                },
                "name": {
                    "type": "string"
                  },
                "price": {
                  "type": "number"
                },
                "quantity": {
                  "type": "number"
                },
                "notes": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            }
          }
        },
        "additionalProperties": false
      },
      "strict": true
    }
  }